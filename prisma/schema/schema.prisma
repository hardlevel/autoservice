generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Autoservice {
  id          Int       @id @default(autoincrement())
  body        String
  receivedAt  String
  createdAt   DateTime  @default(now())
  bodyMd5     String
  messageId   String
  receipt     String
  processedAt DateTime?
}

model LastSearch {
  id         Int      @id @default(1)
  startDate  DateTime
  endDate    DateTime
  status     Boolean
  created_at DateTime @updatedAt
}

