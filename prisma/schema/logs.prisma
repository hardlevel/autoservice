model ErrorLogger {
    id           BigInt   @id @default(autoincrement()) @db.BigInt
    created_at   DateTime @default(now())
    time         DateTime @default(now())
    category     String?
    message      String?
    code         String?
    params       String?
    originalData String?

    // @@unique(name: "error_log", [category, message])
}

model CkLogs {
    id         BigInt   @id @default(autoincrement()) @db.BigInt
    created_at DateTime @default(now())
    startDate  String
    endDate    String
    category   String
    data       String?
    qtd        Int?
    status     String
    message    String?
    jobId      BigInt
}

model JobLogs {
    id         BigInt   @id @default(autoincrement()) @db.BigInt
    jobId      BigInt
    created_at DateTime @default(now())
    started_at DateTime
    ended_at   DateTime
    startDate  String?
    endDate    String?
    status     String
    message    String?
    data       String?
}

model DailyCk {
    id     BigInt @id @default(autoincrement()) @db.BigInt
    day    Int
    month  Int
    year   Int
    ck3001 Int?
    ck3002 Int?
    ck3003 Int?
    ck4001 Int?
    ck5001 Int?
    ck6011 Int?
    ck6021 Int?
    ck6031 Int?
    ck6041 Int?
    ck6042 Int?
    ck7001 Int?
    ck7002 Int?
    ck7003 Int?
    ck7004 Int?
    status String @default("PENDING")

    @@unique(name: "daily", [day, month, year])
}

model LastSearch {
    id        Int    @unique @default(1)
    startDate String
    endDate   String
}

model LastParams {
    day    Int
    month  Int
    year   Int     @unique
    hour   Int
    status Boolean @default(false)
}
