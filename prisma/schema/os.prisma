model tb_cad_cadastro_os {
    id_cadastro      BigInt                                @id @default(autoincrement()) @db.BigInt
    os               String                                @unique //ck6031 por que dois campos de id? mudei de int para string pois os valors que vem da api s√£o string
    id_status_os     BigInt? //ck6031 de onde vem? *
    nfs              tb_cad_cadastro_os_nfs[]
    servicos         tb_cad_cadastro_os_servicos[]
    clientes         tb_cad_cadastro_os_clientes[]
    chassis          tb_cad_cadastro_os_chassis[]
    fontes_pagadoras tb_cad_cadastro_os_fontes_pagadoras[]
    pecas            tb_cad_cadastro_os_pecas[]
    cancelamentos    tb_cad_cadastro_nfs_os_cancelamento[]
}

model tb_cad_cadastro_os_nfs {
    id_cadastro BigInt?
    id                                              BigInt                                         @id @default(autoincrement()) @db.BigInt
    id_nf                                           BigInt //ck
    nf                                              tb_cad_cadastro_nfs                            @relation(fields: [id_nf], references: [id])
    serie_nf                                        String //ck6011
    id_fonte_pagadora                               BigInt? @db.BigInt //ck6011 ok
    fonte_pagadora                                  tb_cad_cadastro_os_fontes_pagadoras?           @relation(fields: [id_fonte_pagadora], references: [id])
    id_seq_nf                                       BigInt? @db.BigInt//desconsiderar
    id_os                                           BigInt @db.BigInt
    os                                              tb_cad_cadastro_os                             @relation(fields: [id_os], references: [id])
    tb_cad_cadastro_nfs_clientes_fontes_pagadoras   tb_cad_cadastro_nfs_clientes_fontes_pagadoras? @relation(fields: [id_fonte_pagadora], references: [id_fonte_pagadora])

    @@unique(name: "nfs_os", [id_nf, os_id])
}
