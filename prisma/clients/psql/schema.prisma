  modified_at                                  DateTime @updatedAt
  ck3002                                       Ck3002[]
  ck3003                                       Ck3003[]

  @@unique([cpf_cnpj, numero_da_nota_fiscal], name: "ck3001_cod")
  @@index([numero_da_nota_fiscal], type: Hash)
}

model Ck3002 {
  id          BigInt      @id @default(autoincrement())
