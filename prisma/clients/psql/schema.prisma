  ck3003                                       Ck3003[]
  created_at                                   DateTime @default(now())
  modified_at                                  DateTime @updatedAt

  @@unique(name: "ck3001_cod", [cpf_cnpj, numero_da_nota_fiscal])
  @@index([numero_da_nota_fiscal], type: Hash)
}

model Ck3002 {
  id          BigInt      @id @default(autoincrement()) @db.BigInt
  cidade      String
  bairro      String?
  uf          String
  cep         String
